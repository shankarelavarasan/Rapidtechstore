{
  "name": "rapid-tech-store",
  "version": "1.0.0",
  "description": "A comprehensive app marketplace platform - PROPRIETARY SOFTWARE",
  "author": "Shankar Elavarasan - Rapid Tech",
  "license": "PROPRIETARY",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:docker\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:docker": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:stop": "docker-compose -f docker-compose.dev.yml down",
    "build": "npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "build:docker": "docker build -t rapid-tech-store .",
    "start": "cd backend && npm start",
    "test": "npm run test:backend",
    "test:backend": "cd backend && npm test",
    "test:watch": "cd backend && npm run test:watch",
    "test:coverage": "cd backend && npm run test:coverage",
    "lint": "cd backend && npm run lint",
    "lint:fix": "cd backend && npm run lint:fix",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:generate": "cd backend && npx prisma generate",
    "db:studio": "cd backend && npx prisma studio",
    "db:seed": "cd backend && npm run seed",
    "db:reset": "cd backend && npx prisma migrate reset",
    "setup:local": "bash scripts/setup-local.sh",
    "deploy:gcp": "bash scripts/deploy.sh",
    "deploy:staging": "bash scripts/deploy.sh --env staging",
    "deploy:production": "bash scripts/deploy.sh --env production",
    "security:scan": "npm audit && cd backend && npm audit",
    "docs:api": "cd backend && npm run docs:generate",
    "clean": "rm -rf node_modules backend/node_modules backend/dist coverage"
  },
  "keywords": [
    "marketplace",
    "app-store",
    "independent-platform",
    "software-distribution",
    "developer-platform",
    "payment-processing",
    "subscription-management",
    "ai-discovery",
    "compliance-automation",
    "global-marketplace",
    "saas-platform",
    "revenue-sharing",
    "digital-commerce"
  ],
  "author": "Rapid Tech Store",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/rapid-tech-store/platform.git"
  },
  "bugs": {
    "url": "https://github.com/rapid-tech-store/platform/issues"
  },
  "homepage": "https://rapidtechstore.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/node": "^20.10.0"
  },
  "workspaces": [
    "backend"
  ],
  "config": {
    "platform": "rapid-tech-store",
    "version": "1.0.0",
    "environment": "development"
  }
}